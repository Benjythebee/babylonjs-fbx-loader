{
  "name": "babylonjs-fbx-loader",
  "version": "0.0.2",
  "description": "An FBX loader for BABYLONJS",
  "lib": "dist/src/index.d.ts",
  "main": "dist/src/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "parcel ./test-scene/index.html --port 9000 --dist-dir ./parcel-dist",
    "test": "cross-env NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.js --no-cache",
    "build": "npm run clean && tsc",
    "prepare": "npm run build",
    "preversion": "npm run format && git add -A src",
    "postversion": "git push origin && git push origin --tags",
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "all": "npm run prepare && npm run version && npm run postversion"
  },
  "keywords": [
    "FBX",
    "loader",
    "babylonjs",
    "threejs"
  ],
  "author": "Benjythebee",
  "license": "ISC",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "directory": "https://github.com/Benjythebee/babylonjs-fbx-loader"
  },
  "bugs": {
    "url": "https://github.com/Benjythebee/babylonjs-fbx-loader/issues"
  },
  "homepage": "https://github.com/Benjythebee/babylonjs-fbx-loader",
  "dependencies": {
    "@babylonjs/core": "^5.24.0",
    "@babylonjs/loaders": "^5.24.0",
    "@babylonjs/materials": "^5.24.0",
    "@picode/binary-reader": "^1.0.0",
    "@types/react": "^18.0.20",
    "buffer-to-data-url": "^1.0.0",
    "fbx-parser": "^2.1.3",
    "jest": "^29.0.3",
    "pako": "^2.0.4",
    "parcel": "^2.7.0",
    "typescript": "^4.8.3",
    "xhr2": "^0.2.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@parcel/transformer-typescript-tsc": "^2.7.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@types/pako": "^2.0.0",
    "assert": "^2.0.0",
    "babel": "^6.23.0",
    "babel-jest": "^29.0.3",
    "buffer": "^6.0.3",
    "constants-browserify": "^1.0.0",
    "cross-env": "^7.0.3",
    "events": "^3.3.0",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.4"
  }
}
